set -e
cd "$(dirname "$0")"
export DISABLE_AUTH=true
export CHANNEL_NAME=mychannel
export CHAINCODE_NAME=carboncc
export MSP_ID=Org1MSP
export PEER_ENDPOINT=127.0.0.1:7051
export PEER_HOST_ALIAS=peer0.org1.example.com
export KEY_PATH=$(ls ~/carbonpasture-kenya/fabric-samples/test-network/organizations/peerOrganizations/org1.example.com/users/*/msp/keystore/* | head -n 1)
export CERT_PATH=~/carbonpasture-kenya/fabric-samples/test-network/organizations/peerOrganizations/org1.example.com/users/*/msp/signcerts/cert.pem
export TLS_CERT_PATH=~/carbonpasture-kenya/fabric-samples/test-network/organizations/peerOrganizations/org1.example.com/peers/peer0.org1.example.com/tls/ca.crt
echo "Starting rest-api with envs:"
env | grep -E 'DISABLE_AUTH|CHANNEL_NAME|CHAINCODE_NAME|MSP_ID|PEER_ENDPOINT|KEY_PATH|TLS_CERT_PATH'
node rest-api.js
